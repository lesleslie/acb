{
  "mcpServers": {
    "session-mgmt": {
      "url": "http://localhost:8678/mcp",
      "type": "http"
    },
    "crackerjack": {
      "url": "http://localhost:8676/mcp",
      "type": "http"
    },
    "macos_automator": {
      "command": "npx",
      "args": [
        "-y",
        "@steipete/macos-automator-mcp@0.4.1"
      ],
      "type": "stdio"
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@cyanheads/git-mcp-server@2.3.3"
      ],
      "type": "stdio"
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory@2025.8.4"
      ],
      "type": "stdio"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking@2025.7.1"
      ],
      "type": "stdio"
    },
    "filesystem": {
      "command": "rust-mcp-filesystem",
      "args": [
        "-w",
        "~/Projects"
      ],
      "type": "stdio"
    },
    "mermaid": {
      "url": "http://localhost:3033/mcp",
      "type": "http"
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@1.0.17"
      ],
      "type": "stdio"
    },
    "excalidraw": {
      "url": "http://localhost:3032/mcp",
      "type": "http"
    },
    "cloud-run": {
      "command": "npx",
      "args": [
        "-y",
        "https://github.com/GoogleCloudPlatform/cloud-run-mcp"
      ],
      "type": "stdio"
    },
    "logfire": {
      "command": "uvx",
      "args": [
        "logfire-mcp@0.6.0"
      ],
      "type": "stdio"
    },
    "sentry": {
      "url": "https://mcp.sentry.dev/mcp",
      "type": "http"
    },
    "turso-cloud": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-turso-cloud@0.0.10"
      ],
      "type": "stdio"
    },
    "upstash": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/mcp-server@0.2.0",
        "run"
      ],
      "type": "stdio"
    }
  }
}
